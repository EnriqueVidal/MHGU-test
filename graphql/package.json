{
  "name": "boilerplate-graphql",
  "version": "0.1.2",
  "description": "Everything you need to start your GraphQL endpoint",
  "main": "src/index.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production npx webpack",
    "check-types": "tsc",
    "coverage": "nyc report --reporter=text-lcov | npx coveralls",
    "lint": "tslint 'src/**/*.ts'",
    "test": "cross-env NODE_ENV=test npx mocha-webpack --recursive",
    "test:coverage": "cross-env MOCHA_ENV=coverage nyc npm test",
    "start": "cross-env NODE_PATH=./src:./config nodemon -e js,ts ./src --exec babel-node --extensions \".js,.ts\""
  },
  "keywords": [
    "boilerplate",
    "graphql",
    "webpack"
  ],
  "author": "Enrique Vidal",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/node": "^10.12.18",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "dotenv-webpack": "^1.6.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "mocha": "^5.2.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "sinon": "^7.2.2",
    "supertest": "^3.3.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "dataloader-sequelize": "^1.7.6",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.0.2",
    "graphql-relay": "^0.5.5",
    "graphql-sequelize": "^9.3.1",
    "lodash": "^4.17.11",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.42.0",
    "sequelize-typescript": "^0.6.6"
  },
  "engines": {
    "node": ">= 11.0.0",
    "npm": ">= 5.4.1"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "instrument": false,
    "sourceMap": false
  }
}
